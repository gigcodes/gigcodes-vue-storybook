{
  "name": "gc-ui",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "preview": "vite preview",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "build:styles": "postcss src/assets/styles/app.css -o dist/gc-ui.css",
    "deploy-storybook": "gh-pages -d storybook-static"
  },
  "main": "dist/gc-ui.umd.js",
  "module": "dist/gc-ui.es.js",
  "exports": {
    ".": {
      "import": "./dist/gc-ui.es.js",
      "require": "./dist/gc-ui.umd.js"
    },
    "./dist/gc-ui.css": {
      "import": "./dist/gc-ui.css",
      "require": "./dist/gc-ui.css"
    }
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "@floating-ui/vue": "^1.0.1",
    "@headlessui/vue": "^1.7.14",
    "@heroicons/vue": "^2.0.17",
    "@storybook/addon-actions": "^7.0.20",
    "@storybook/addon-knobs": "^7.0.2",
    "@storybook/addon-styling": "^1.3.0",
    "@tailwindcss/typography": "^0.5.9",
    "classnames": "^2.3.2",
    "cssnano": "^6.0.1",
    "dayjs": "^1.11.7",
    "floating-vue": "^2.0.0-beta.20",
    "fuse.js": "^6.6.2",
    "lodash": "^4.17.21",
    "mitt": "^3.0.0",
    "motion": "^10.15.5",
    "postcss": "^8.4.23",
    "postcss-import": "^15.1.0",
    "postcss-nesting": "^11.2.2",
    "postcss-preset-env": "^8.3.2",
    "rollup-plugin-typescript2": "^0.34.1",
    "speakingurl": "^14.0.1",
    "tailwind-safelist-generator": "^1.0.0",
    "twin.macro": "^3.3.1",
    "vite-plugin-dts": "^2.3.0",
    "vue": "^3.2.47",
    "vue-tsc": "^1.6.5",
    "vue-types": "^5.0.3"
  },
  "devDependencies": {
    "@prettier/plugin-php": "^0.19.4",
    "@storybook/addon-essentials": "^7.0.20",
    "@storybook/addon-interactions": "^7.0.20",
    "@storybook/addon-links": "^7.0.20",
    "@storybook/addon-mdx-gfm": "^7.0.20",
    "@storybook/addon-postcss": "^2.0.0",
    "@storybook/blocks": "^7.0.20",
    "@storybook/testing-library": "^0.1.0",
    "@storybook/vue3": "^7.0.20",
    "@storybook/vue3-vite": "^7.0.20",
    "@vitejs/plugin-vue": "^4.1.0",
    "@vue-macros/volar": "^0.10.2",
    "autoprefixer": "^10.4.14",
    "eslint": "^8.41.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-storybook": "^0.6.12",
    "eslint-plugin-vue": "^9.14.1",
    "gh-pages": "^5.0.0",
    "prettier": "^2.8.8",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "storybook": "^7.0.20",
    "tailwindcss": "^3.3.1",
    "unplugin-vue-define-options": "^1.3.7",
    "unplugin-vue-macros": "^2.2.1",
    "vite": "^4.3.0"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "browser": true,
      "node": true
    },
    "extends": [
      "eslint:recommended",
      "plugin:vue/vue3-recommended",
      "prettier",
      "plugin:storybook/recommended"
    ],
    "plugins": [
      "prettier"
    ],
    "rules": {
      "prettier/prettier": [
        "error"
      ],
      "vue/require-default-prop": 0,
      "vue/html-indent": [
        "error",
        4
      ],
      "vue/singleline-html-element-content-newline": 0,
      "vue/component-name-in-template-casing": [
        "error",
        "PascalCase"
      ],
      "vue/multi-word-component-names": "off"
    },
    "globals": {
      "_": true
    }
  }
}
